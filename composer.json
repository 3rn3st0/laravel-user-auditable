{
    "name": "ernestoch/laravel-user-auditable",
    "description": "Laravel package for user auditing in migrations and models",
    "type": "library",
    "keywords": ["laravel", "audit", "user", "migration", "model", "tracking"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ernesto Chapon",
            "email": "ernestochapon@gmail.com",
            "homepage": "https://github.com/3rn3st0"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.36",
        "phpunit/phpunit": "^10.0",
        "nunomaduro/collision": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ErnestoCh\\UserAuditable\\": "src/",
            "ErnestoCh\\UserAuditable\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ErnestoCh\\UserAuditable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ErnestoCh\\UserAuditable\\Providers\\UserAuditableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "scripts": {
        "release:patch": "git tag v$(php -r \"echo explode('.', file_get_contents('composer.json'))[2]\") && git push --tags",
        "release:minor": "echo 'Update version in composer.json then run release:patch'",
        "test": "vendor\\bin\\phpunit",
        "test-coverage": "vendor\\bin\\phpunit --coverage-html coverage",
        "test-verbose": "vendor\\bin\\phpunit --verbose"
    }
}
